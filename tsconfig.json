{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ESNext",
    "types": [],
    "strict": true,
    "declaration": true,
    "esModuleInterop": true,
    "inlineSourceMap": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "strictPropertyInitialization": true,
    "experimentalDecorators": true,
    "strictFunctionTypes": true,
    "skipLibCheck": true,
    "incremental": true
  },
  "exclude": [
    "node_modules"
  ]
}
