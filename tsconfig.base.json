{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es2017",
        "jsx": "react",
        "lib": ["esnext"],
        "strict": true,
        "declaration": true,
        "inlineSourceMap": true,
        "strictNullChecks": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "strictPropertyInitialization": true,
        "strictFunctionTypes": true,
        "experimentalDecorators": true,
        "skipLibCheck": true,
        "incremental": true
    }
}
