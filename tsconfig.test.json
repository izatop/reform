{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ES2019",
        "jsx": "react",
        "lib": ["esnext", "dom", "dom.iterable"],
        "types": ["jest"],
        "esModuleInterop": true,
        "sourceMap": true,
        "strict": true,
        "strictNullChecks": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitThis": true,
        "noImplicitAny": true,
        "strictPropertyInitialization": true,
        "strictFunctionTypes": true,
        "experimentalDecorators": true,
        "noEmit": true
    }
}
